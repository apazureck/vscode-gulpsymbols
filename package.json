{
    "name": "gulpsymbols",
    "displayName": "Gulp Task Symbol Provider",
    "description": "Adds gulp tasks to the symbol provider in your gulpfile.js",
    "version": "0.1.1",
    "publisher": "apazureck",
    "author": {
        "email": "andreas@pazureck.de",
        "name": "Andreas Pazureck"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/apazureck/vscode-gulpsymbols"
    },
    "icon": "gulplogo.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}